#!/bin/bash --login
#PBS -l walltime=01:00:00
#PBS -l select=1:ncpus=32
#PBS -N 2018_normalize_table
#PBS -A d411-training
cd $PBS_O_WORKDIR
export TMPDIR=~/qiime_tmp
module load miniconda/python2
module load R-3.3.1-gcc-6.2.0-zjrjm3s
echo "loading virtualenv"
source activate qiime1
echo "Normalizing OTU table"
normalize_table.py -i ~/2018_02_smb/Pick_Closed_Reference_OTUs_25/otu_table.biom -a CSS -o ~/2018_02_smb/Pick_Closed_Reference_OTUs_25/CSS_normalized_otu_table.biom
source deactivate
